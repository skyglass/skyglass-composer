{
  "swagger" : "2.0",
  "info" : {
    "version" : "1.1.1",
    "title" : "My API"
  },
  "host" : "127.0.0.1:8080",
  "basePath" : "/",
  "tags" : [ {
    "name" : "doctors",
    "description" : "the doctors API"
  } ],
  "schemes" : [ "http" ],
  "paths" : {
    "/doctors/" : {
      "get" : {
        "tags" : [ "doctors" ],
        "summary" : "Retrieve all doctors",
        "description" : "",
        "operationId" : "getAllDoctors",
        "produces" : [ "application/xml" ],
        "responses" : {
          "200" : {
            "description" : "Successfully retrieved Doctors",
            "schema" : {
              "type" : "array",
              "items" : {
                "$ref" : "#/definitions/DoctorDto"
              }
            }
          },
          "400" : {
            "description" : "Invalid ID supplied"
          },
          "404" : {
            "description" : "Doctor with specified ID not found"
          }
        }
      },
      "post" : {
        "tags" : [ "doctors" ],
        "summary" : "Add a new doctor to Medical Clinic",
        "description" : "",
        "operationId" : "createDoctor",
        "consumes" : [ "application/xml", "application/json" ],
        "produces" : [ "application/xml", "application/json" ],
        "parameters" : [ {
          "in" : "body",
          "name" : "body",
          "description" : "Doctor object that needs to be added to the clinic",
          "required" : true,
          "schema" : {
            "$ref" : "#/definitions/DoctorDto"
          }
        } ],
        "responses" : {
          "200" : {
            "description" : "successful operation",
            "schema" : {
              "$ref" : "#/definitions/DoctorDto"
            }
          },
          "201" : {
            "description" : "Created Doctor",
            "schema" : {
              "$ref" : "#/definitions/DoctorDto"
            }
          },
          "405" : {
            "description" : "Invalid input"
          }
        }
      }
    },
    "/doctors/{id}" : {
      "get" : {
        "tags" : [ "doctors" ],
        "summary" : "Retrieve existing doctor information",
        "description" : "",
        "operationId" : "getDoctor",
        "produces" : [ "application/xml" ],
        "parameters" : [ {
          "name" : "id",
          "in" : "path",
          "description" : "Id of doctor that needs to be retrieved",
          "required" : true,
          "type" : "integer",
          "format" : "int64"
        } ],
        "responses" : {
          "200" : {
            "description" : "Successfully retrieved Doctor",
            "schema" : {
              "$ref" : "#/definitions/DoctorDto"
            }
          },
          "400" : {
            "description" : "Invalid ID supplied"
          },
          "404" : {
            "description" : "Doctor with specified ID not found"
          }
        }
      }
    },
    "/swagger-resources" : {
      "get" : {
        "operationId" : "swaggerResources",
        "produces" : [ "application/json" ],
        "responses" : {
          "200" : {
            "description" : "successful operation",
            "schema" : {
              "type" : "object"
            }
          }
        }
      }
    },
    "/swagger-resources/configuration/security" : {
      "get" : {
        "operationId" : "swagerSecurityConfig",
        "produces" : [ "application/json" ],
        "responses" : {
          "200" : {
            "description" : "successful operation",
            "schema" : {
              "type" : "object"
            }
          }
        }
      }
    },
    "/swagger-resources/configuration/ui" : {
      "get" : {
        "operationId" : "swaggerUiConfig",
        "produces" : [ "application/json" ],
        "responses" : {
          "200" : {
            "description" : "successful operation",
            "schema" : {
              "type" : "object"
            }
          }
        }
      }
    },
    "/v2/api-docs" : {
      "get" : {
        "operationId" : "apiJsonEndpoint",
        "produces" : [ "application/json" ],
        "responses" : {
          "200" : {
            "description" : "successful operation",
            "schema" : {
              "$ref" : "#/definitions/Resource"
            }
          }
        }
      }
    }
  },
  "definitions" : {
    "DoctorDto" : {
      "type" : "object",
      "properties" : {
        "entityId" : {
          "type" : "integer",
          "format" : "int64"
        },
        "name" : {
          "type" : "string"
        },
        "links" : {
          "readOnly" : true,
          "$ref" : "#/definitions/Links"
        }
      },
      "xml" : {
        "name" : "DoctorDto"
      }
    },
    "InputStream" : {
      "type" : "object"
    },
    "Links" : {
      "type" : "object",
      "properties" : {
        "empty" : {
          "type" : "boolean"
        }
      }
    },
    "Resource" : {
      "type" : "object",
      "properties" : {
        "url" : {
          "type" : "string",
          "format" : "url"
        },
        "file" : {
          "type" : "file"
        },
        "open" : {
          "type" : "boolean"
        },
        "readable" : {
          "type" : "boolean"
        },
        "uri" : {
          "type" : "string",
          "format" : "uri"
        },
        "description" : {
          "type" : "string"
        },
        "filename" : {
          "type" : "string"
        },
        "inputStream" : {
          "$ref" : "#/definitions/InputStream"
        }
      }
    }
  }
}